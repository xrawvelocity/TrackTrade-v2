    "@mui/icons-material": "^5.1.1",
    "@mui/lab": "^5.0.0-alpha.57",
    "@mui/material": "^5.11.6",
    "@mui/styles": "^5.1.1",
    "@mui/x-data-grid": "^5.0.1",
    "axios": "^1.6.7",
    "chart.js": "^3.6.0",
    "ci": "^2.1.1",
    "concat": "^1.0.3",
    "dotenv": "^16.4.4",
    "final-form": "^4.20.4",
    "firebase": "^10.8.0",
    "get-contrast": "^3.0.0",
    "moment": "^2.29.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.7",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^5.0.1",
    "react-share": "^4.4.0",
    "recharts": "^2.1.6",
    "sass": "^1.32.12",
    "styled-components": "^5.3.3"
  },
  "scripts": {
    "watch:sass": "sass src/sass/main.scss src/css/style.css -w",
    "compile:sass": "sass src/sass/main.scss src/css/style.comp.css",
    "concat:css": "concat -o src/css/style.concat.css src/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' src/css/style.concat.css -o src/css/style.prefix.css",
    "compress:css": "sass src/css/style.prefix.css src/css/style.css --style=compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
